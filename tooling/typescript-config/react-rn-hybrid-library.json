{
  "display": "Hybrid UI Library (React + React Native)",
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es2018",
    "lib": ["DOM", "ESNext"],
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "bundler"
  }
}
