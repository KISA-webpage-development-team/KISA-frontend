{
  "display": "Hybrid UI Library (React + React Native)",
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "noEmit": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "types": ["react", "react-native"]
  }
}
