{
  "display": "Expo React Native",
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2020",
    "skipLibCheck": true
  },
  "include": ["src", "app", "index.ts", "index.tsx", "App.tsx"],
  "exclude": ["node_modules", "dist"]
}
